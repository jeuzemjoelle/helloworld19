pipeline {
  agent any
  stages {
    stage('Build'){
      steps {
        echo "build step"
        sleep 10
      }
    }
    stage('Test'){
      steps {
        echo "Test step"
        sleep 10
      }
    }
    stage('Deploy'){
      steps {
        echo "deploy step"
        sleep 10
      }
    }
    stage('Docker'){
      steps {
        echo "image step"
        sleep 10
      }
    }
  }
}
